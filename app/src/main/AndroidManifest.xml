<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.qamber.carpiker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.MainActivity" />
        <activity android:name=".Activity.Login" />
        <activity android:name=".Activity.SignUp" />
        <activity android:name=".Activity.MainFragmentActivity" />
        <activity android:name=".Activity.EnterPhone" />
        <activity android:name=".Activity.EnterCode"></activity>
    </application>

</manifest>